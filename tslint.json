{
  "extends": "tslint-microsoft-contrib/tslint.json",
  "rulesDirectory": [
    "node_modules/tslint-microsoft-contrib"
  ],
  "rules": {
    "no-for-in-array": false,
    "restrict-plus-operands": false,
    "no-unused-variable": [true, "react"],
    "missing-jsdoc": false,
    "no-relative-imports": false,
    "quotemark": false
  }
}
